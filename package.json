  "bugs": {
    "url": "https://github.com/SteffenKn/7-segment-timer.client/issues"
  },
  "homepage": "https://github.com/SteffenKn/7-segment-timer.client#readme",
  "dependencies": {
    "7-segment-timer.contracts": "0.0.1-alpha.2",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.11",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.6",
    "@types/node-fetch": "^2.5.7",
    "c8": "7.2.0",
    "chai": "4.2.0",
    "mocha": "8.0.1",
    "tslint": "6.1.2",
    "typescript": "3.9.5"
  }
